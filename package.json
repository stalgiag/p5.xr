{
  "name": "p5.xr",
  "version": "0.5.2",
  "description": "A library for p5.js that allows coders to easily render p5.js sketches in AR/VR using WebXR",
  "source": "./src/app.js",
  "unpkg": "dist/p5xr.min.js",
  "p5xr.min": "dist/p5xr.min.js",
  "p5xr": "dist/p5xr.js",
  "targets": {
    "p5xr.min": {
      "optimize": true,
      "isLibrary": false,
      "outputFormat": "global"
    },
    "p5xr": {
      "optimize": false,
      "isLibrary": false,
      "outputFormat": "global"
    }
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "grunt": "grunt build",
    "build": "parcel build --no-cache --no-source-maps",
    "watch": "parcel watch",
    "test": "karma start karma.conf.js",
    "docs": "npx docsify serve docs",
    "setup-ssl": "npm run generate-privatekey && npm run generate-csr && npm run generate-cert",
    "generate-privatekey": "openssl genrsa -out key.pem",
    "generate-csr": "openssl req -new -key key.pem -out csr.pem",
    "generate-cert": "openssl x509 -req -days 365 -in csr.pem -signkey key.pem -out cert.pem",
    "dev-server": "http-server -S -C cert.pem -K key.pem -o",
    "jsdoc2md": "grunt jsdoc2md",
    "release": "commit-and-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stalgiag/p5.xr.git"
  },
  "keywords": [
    "creativecoding",
    "ar",
    "vr",
    "xr"
  ],
  "author": "Stalgia Grigg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stalgiag/p5.xr/issues"
  },
  "homepage": "https://github.com/stalgiag/p5.xr#readme",
  "dependencies": {
    "@swc/helpers": "^0.4.2",
    "gl-matrix": "^3.4.3",
    "quaternion": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/runtime": "^7.18.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
    "chai": "^4.3.6",
    "docdash": "^1.2.0",
    "docsify-cli": "^4.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "fast-async": "^6.3.8",
    "grunt": "^1.5.3",
    "grunt-eslint": "^24.0.0",
    "grunt-jsdoc-to-markdown": "^6.0.0",
    "grunt-karma": "^4.0.2",
    "grunt-run": "^0.8.1",
    "http-server": "^14.1.1",
    "jsdoc": "^3.6.10",
    "jsdoc-export-default-interop": "^0.3.1",
    "jsdoc-to-markdown": "^7.1.1",
    "karma": "^6.4.1",
    "karma-browserify": "^8.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "mocha": "^10.2.0",
    "openssl": "^2.0.0",
    "p5": "~1.10.0",
    "parcel": "^2.6.2",
    "sinon": "^7.5.0",
    "commit-and-tag-version": "^12.4.3",
    "watchify": "^4.0.0"
  }
}
