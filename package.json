{
  "name": "p5.xr",
  "version": "0.0.1",
  "description": "A library for p5.js that allows users to easily render p5.js sketches in AR/VR using WebXR",
  "main": "src/p5xr.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "parcel build ./src/app.js --no-minify --out-file p5xr.js",
    "watch": "parcel watch ./src/app.js --out-file p5xr.js",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stalgiag/p5.xr.git"
  },
  "keywords": [
    "creativecoding",
    "ar",
    "vr"
  ],
  "author": "Stalgia Grigg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stalgiag/p5.xr/issues"
  },
  "homepage": "https://github.com/stalgiag/p5.xr#readme",
  "dependencies": {
    "p5": "^0.8.0",
    "webxr-polyfill": "^1.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "*",
    "docdash": "^1.0.3",
    "eslint": "^5.15.3",
    "jsdoc": "^3.5.5",
    "jsdoc-export-default-interop": "^0.3.1",
    "karma": "^4.1.0",
    "karma-browserify": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "*",
    "parcel-bundler": "^1.12.3",
    "watchify": "^3.11.1"
  }
}
