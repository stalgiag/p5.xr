{
  "name": "p5.xr",
  "version": "0.4.0",
  "description": "A library for p5.js that allows coders to easily render p5.js sketches in AR/VR using WebXR",
  "main": "dist/p5xr.js",
  "unpkg": "dist/p5xr.min.js",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "parcel build ./src/app.js --no-minify --no-source-maps --out-file p5xr.js",
    "build:min": "parcel build ./src/app.js --no-source-maps --out-file p5xr.min.js",
    "watch": "parcel watch ./src/app.js --no-source-maps --out-file  p5xr.js",
    "test": "karma start karma.conf.js",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json -r",
    "generate:keys": "openssl req -newkey rsa:2048 -new -nodes -x509 -days 3650 -keyout key.pem -out cert.pem ",
    "dev:server": "http-server -S -C cert.pem -K key.pem -o",
    "prerelease": "npm run build && npm run build:min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stalgiag/p5.xr.git"
  },
  "keywords": [
    "creativecoding",
    "ar",
    "vr",
    "xr"
  ],
  "author": "Stalgia Grigg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stalgiag/p5.xr/issues"
  },
  "homepage": "https://github.com/stalgiag/p5.xr#readme",
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "webxr-polyfill": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "chai": "*",
    "docdash": "^1.2.0",
    "docsify-cli": "^4.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "fast-async": "^6.3.8",
    "http-server": "^0.12.3",
    "jsdoc": "^3.6.4",
    "jsdoc-export-default-interop": "^0.3.1",
    "karma": "^6.3.14",
    "karma-browserify": "^6.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "mocha": "^6.2.3",
    "openssl": "^1.1.0",
    "p5": "^1.4.0",
    "parcel-bundler": "^1.12.5",
    "sinon": "^7.5.0",
    "watchify": "^3.11.1"
  }
}
